{{ if ne .chezmoi.os "windows" -}}
#!/usr/bin/env fish

echo -e "\033[0;32m>>>>> Begin Setting Fish Variables <<<<<\033[0m"

set --universal LC_ALL en_US.UTF-8
set --universal LANG en_US.UTF-8

set --universal nvm_default_version lts
